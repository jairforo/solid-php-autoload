{
    "name": "jair.foro/recharge",
    "description": "Quick start for a PHP application with PHPunit and autoload using a bit of SOLID",
    "require": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3"
    },
    "authors": [
        {
            "name": "Jair Foro",
            "email": "jairforo@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "App\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "App\\Tests\\": "tests"
      }
    }
}
